{
  "name": "solana-dex-modern",
  "description": "Modern DEX on Solana with concentrated liquidity, Jupiter integration, and advanced trading features",
  "keywords": [
    "solana",
    "dex",
    "defi",
    "anchor",
    "jupiter",
    "concentrated-liquidity",
    "nextjs",
    "react",
    "tailwind",
    "typescript",
    "trading",
    "amm",
    "liquidity-pools"
  ],
  "create-solana-dapp": {
    "instructions": [
      "To configure the Anchor deployment keypair, run this:",
      "+{pm} run setup",
      "Run Anchor commands:",
      "+{pm} run anchor build | test | localnet | deploy"
    ],
    "rename": {
      "basic": {
        "to": "{{name}}",
        "paths": [
          "anchor",
          "src"
        ]
      },
      "scaffold": {
        "to": "{{name}}",
        "paths": [
          "src"
        ]
      }
    }
  },
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "anchor": "cd anchor && anchor",
    "anchor-build": "cd anchor && anchor build",
    "anchor-localnet": "cd anchor && anchor localnet",
    "anchor-test": "cd anchor && anchor test",
    "anchor-deploy": "cd anchor && anchor deploy",
    "build": "next build",
    "codama:js": "codama run js -c ./anchor/codama.js",
    "ci": "npm run build && npm run lint && npm run format:check && npm run codama:js",
    "dev": "next dev --turbopack",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "setup": "npm run anchor keys sync && npm run codama:js",
    "setup-env": "node scripts/setup-env.js",
    "setup-dev": "npm run setup-env && npm run setup",
    "setup-prod": "npm run anchor-build && npm run anchor-deploy && npm run codama:js",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-progress": "^1.0.3",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.84.1",
    "@wallet-ui/react": "1.1.0-canary-20250617152337",
    "@wallet-ui/tailwind": "1.1.0-canary-20250617152337",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "codama": "^1.3.1",
    "framer-motion": "^11.11.17",
    "gill": "0.10.2",
    "jotai": "^2.12.5",
    "lightweight-charts": "^4.2.0",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "recharts": "^2.13.3",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.6",
    "@jup-ag/api": "^6.0.30",
    "@solana/spl-token": "^0.4.8",
    "bignumber.js": "^9.1.2",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@codama/cli": "^1.2.1",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "esrun": "^3.2.30",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  }
}
